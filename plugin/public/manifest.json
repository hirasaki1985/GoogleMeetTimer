{
  "name": "GoogleMeetTimer",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Timer on Google Meet.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "html": ["index.html"]
    }
  ]
}